{
  "name": "ng-three-model-cropper",
  "version": "1.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:delay": "powershell -c \"Start-Sleep -Seconds 6; npm start\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:lib": "ng build model-cropper --configuration production",
    "watch:lib": "ng build model-cropper --watch --configuration development",
    "dev": "npm run build:lib && concurrently \"npm run watch:lib\" \"npm run start:delay\" --names \"LIB,APP\" --prefix-colors \"blue,green\"",
    "publish:lib": "npm run build:lib && cd dist/model-cropper && npm publish",
    "test:lib": "ng test model-cropper --browsers=ChromeHeadless --watch=false",
    "test:lib:watch": "ng test model-cropper",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"projects/**/*.{ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"projects/**/*.{ts,html,scss,css,json}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "~7.8.0",
    "three": "^0.162.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/three": "^0.162.0",
    "angular-eslint": "21.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "prettier": "^3.7.4",
    "typescript": "~5.3.2",
    "typescript-eslint": "8.46.4"
  }
}
